[run]
omit =
    docs/*
    tests/*
    ads/automl/*
    ads/common/artifact/*
    ads/environment/*
    ads/experiments/*
    ads/templates/*
    */constants.py
    */_imports.py
    */__init__.py
    */__main__.py
    */.*
    /private/*
    /tmp/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
omit =
    ads/automl/*
    ads/common/artifact/*
    ads/environment/*
    ads/experiments/*
    ads/templates/*
    */constants.py
    */_imports.py
    */__init__.py
    */__main__.py
    __init.py__
    /private/*
    /tmp/*
show_missing = true
skip_empty = true
precision = 2

[html]
directory = htmlcov
